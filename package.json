{
  "name": "snyk-api",
  "version": "0.0.1-0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "snyk-api": "./dist/index.js"
  },
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "test": "NODE_ENV=test mocha --timeout 300000 --recursive --exit -r ts-node/register 'test/**/*.test.ts'",
    "test-groups": "NODE_ENV=test mocha --timeout 300000 --recursive --exit -r ts-node/register 'test/commands/process/groups*.test.ts'",
    "test-general": "NODE_ENV=test mocha --timeout 300000 --recursive --exit -r ts-node/register 'test/commands/process/general*.test.ts'",
    "test-integrations": "NODE_ENV=test mocha --timeout 300000 --recursive --exit -r ts-node/register 'test/commands/process/integrations*.test.ts'",
    "test-orgs": "NODE_ENV=test mocha --timeout 300000 --recursive --exit -r ts-node/register 'test/commands/process/organizations*.test.ts'",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint src/**/*.ts -p tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "configstore": "^5.0.1",
    "debug": "^4.3.1",
    "ora": "^5.3.0",
    "prompts": "^2.4.0",
    "snyk-api-client": "^0.0.1-2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/configstore": "^4.0.0",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.28",
    "@types/prompts": "^2.0.9",
    "@types/yargs": "^16.0.0",
    "chai": "^4.3.0",
    "mocha": "^8.3.0",
    "prettier": "^2.2.1",
    "snyk": "^1.456.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.5"
  }
}